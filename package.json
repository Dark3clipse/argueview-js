{
	"name": "argueview-js",
	"version": "0.5.0",
	"private": false,
	"description": "ArgueView is a tool for generating text-based presentations for machine-learning predictions and feature-importance based explanation tools. The tool makes use of Toulmin's model of argumentation for structuring the text-based explanations. This javascript contains visualization components that can be used to render explanations generated by the Python counterpart of ArgueView.",
	"repository": "https://github.com/SophiaHadash/argueview-js",
	"scripts": {
		"build": "sh build/prebuild.sh && webpack --config build/webpack.dev.js && webpack --config build/webpack.prod.js && tsc --declaration --emitDeclarationOnly --outDir dist/typings",
		"start": "cp test/explanation.json test/manual/src/explanation.json && cp test/explanation_negative.json test/manual/src/explanation_negative.json && cp test/explanation_exceptions.json test/manual/src/explanation_exceptions.json && cd test/manual && npm start" ,
		"test": "jest"
	},
	"main": "dist/argueview",
	"unpkg": "dist/argueview.js",
	"types": "dist/typings/loader.d.ts",
	"files": [
		"dist"
	],
	"keywords": [
		"explanations",
		"text",
		"toulmin",
		"argumentation"
	],
	"author": {
		"name": "Sophia Hadash, MSc",
		"email": "s.hadash@tue.nl",
		"url": "https://github.com/SophiaHadash"
	},
	"license": "GPL-3.0",
	"jest-junit": {
		"outputDirectory": "./coverage",
		"outputName": "junit.xml"
	},
	"dependencies": {
		"react": "^17.0.1",
		"react-line-awesome": "^1.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-decorators": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/plugin-transform-modules-commonjs": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"@babel/preset-typescript": "^7.12.7",
		"@jest/globals": "^26.6.2",
		"@types/jest": "^26.0.18",
		"@types/node": "^14.14.11",
		"@types/react-dom": "^16.9.10",
		"@types/react-router": "^5.1.8",
		"@types/react-router-dom": "^5.1.6",
		"@types/webpack-env": "^1.16.0",
		"autoprefixer": "^10.1.0",
		"babel-loader": "^8.2.2",
		"babel-plugin-strip-decorator": "^1.0.0",
		"babel-plugin-transform-typescript-metadata": "^0.3.1",
		"babel-preset-minify": "^0.5.1",
		"babel-preset-mobx": "^2.0.0",
		"copy-webpack-plugin": "^6.4.0",
		"css-loader": "^5.0.1",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.0",
		"html-webpack-tags-plugin": "^2.0.17",
		"identity-obj-proxy": "^3.0.0",
		"imports-loader": "^1.2.0",
		"jest": "^26.6.3",
		"jest-junit": "^12.0.0",
		"mini-css-extract-plugin": "^1.3.2",
		"node-sass": "^5.0.0",
		"postcss-loader": "^4.1.0",
		"postcss-scss": "^3.0.4",
		"react-dom": "^17.0.1",
		"sass": "^1.30.0",
		"sass-loader": "^10.1.0",
		"source-map-support": "^0.5.19",
		"style-loader": "^2.0.0",
		"ts-jest": "^26.4.4",
		"ts-loader": "^8.0.11",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.1.2",
		"url-loader": "^4.1.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0",
		"webpack-merge": "^5.4.1",
		"webpack-node-externals": "^2.5.2"
	}
}
